{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "noImplicitReturns": true
  },
  "include": [
    "vite.config.ts"
  ]
}